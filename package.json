{
  "name": "proworkflow-dashboard",
  "version": "1.0.0",
  "description": "Live dashboard for ProWorkflow data using REST and GraphQL APIs",
"main": "app.js",
  "scripts": {
"start": "node app.js",
    "start:dashboard": "node dashboard-app.js",
    "start:queue": "node assignment-queue-app.js",
    "start:both": "concurrently \"npm run start:dashboard\" \"npm run start:queue\"",
    "dev": "concurrently \"nodemon dashboard-app.js\" \"nodemon assignment-queue-app.js\"",
    "dev:dashboard": "nodemon dashboard-app.js",
    "dev:queue": "nodemon assignment-queue-app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "git push heroku main"
  },
  "keywords": [
    "proworkflow",
    "dashboard",
    "api",
    "graphql",
    "rest",
    "heroku",
    "assignment-queue"
  ],
